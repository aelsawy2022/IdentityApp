@model IEnumerable<IdentityApplication.Data.Entities.Governorate>
@{
    ViewData["Title"] = "Index";
}

<div class="text-center">
    <h1 class="display-4">Governorates</h1>
    <p>You can manage governorates from this screen.</p>
</div>

<div class="row">
    <div class="col-12">
        &nbsp;
    </div>
</div>

<a asp-action="Create">Create</a>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Creation Date</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role.Id</td>
                <td>@role.Name</td>
                <td>@role.CreationDate.ToString("MM/dd/yyyy")</td>
            </tr>

        }
    </tbody>
</table> 