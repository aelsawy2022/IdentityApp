@*@model SchoolManagement.ViewModels.ViewModels.ActivityVM*@
@model List<ActivitySlotModel>

@if (Model != null && Model.Count > 0)
{
    for (int i = 0; i < Model.Count; i++)
    {
        <div class="form-group">
            <label asp-for="@Model[i].Slot" class="control-label"></label>
            <input asp-for="@Model[i].Slot" class="form-control" />
            <span asp-validation-for="@Model[i].Slot" class="text-danger"></span>
        </div>
    }
}